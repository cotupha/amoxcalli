@article{Cevolani:TruthApproximationAbductiveBeliefChange,
	abstract = "We investigate the logical and conceptual connections between abductive reasoning construed as a process of belief change, on the one hand, and truth approximation, construed as increasing (estimated) verisimilitude, on the other. We introduce the notion of {\lq}(verisimilitude-guided) abductive belief change{\rq} and discuss under what conditions abductively changing our theories or beliefs does lead them closer to the truth, and hence tracks truth approximation conceived as the main aim of inquiry. The consequences of our analysis for some recent discussions concerning belief revision aiming at truth approximation and inference to the best explanation are also highlighted.",
	author = "Cevolani, Gustavo",
	doi = "10.1093/jigpal/jzt010",
	eprint = "http://jigpal.oxfordjournals.org/content/early/2013/03/20/jigpal.jzt010.full.pdf+html",
	journal = "Logic Journal of IGPL",
	note = "Advanced Access published March 20th, 2013",
	title = "{Truth approximation via abductive belief change}",
	url = "http://jigpal.oxfordjournals.org/content/early/2013/03/20/jigpal.jzt010.abstract",
	year = "2013"
}

